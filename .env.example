# Database Configuration
MONGODB_URI=mongodb://localhost:27017/eo_dubai_events

# Server Configuration
NODE_ENV=development
PORT=3001
FRONTEND_URL=http://localhost:8000

# Telr Payment Gateway Configuration
TELR_STORE_ID=your_telr_store_id
TELR_AUTH_KEY=your_telr_auth_key
TELR_BASE_URL=https://secure.telr.com/gateway/order.json

# Google Sheets API Configuration
GOOGLE_SERVICE_ACCOUNT_KEY_PATH=./google-service-account-key.json

# Honeycommb Webhook Configuration
HONEYCOMMB_WEBHOOK_SECRET=your_webhook_secret

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_here

# Email Configuration (Resend)
RESEND_API_KEY=your_resend_api_key
RESEND_FROM_EMAIL=events@eodubai.com

# Admin Configuration
ADMIN_EMAIL=admin@eodubai.com
ADMIN_PASSWORD=secure_admin_password
